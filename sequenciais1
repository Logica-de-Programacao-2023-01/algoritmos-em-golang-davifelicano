package main

import "fmt"

func main() {
	var numero1 int
	fmt.Println("Digite um numero: ")
	fmt.Scan(&numero1)
	var numero2 int
	fmt.Println("Digite um numero: ")
	fmt.Scan(&numero2)
	var numero3 int
	fmt.Println("Digite um numero: ")
	fmt.Scan(&numero3)
	print("Soma:", numero1+numero2+numero3)
}
