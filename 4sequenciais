	var numero1 int
	fmt.Print("Digite um numero: ")
	fmt.Scan(&numero1)
	var numero2 int
	fmt.Print("Digite um numero: ")
	fmt.Scan(&numero2)
	var numero3 int
	fmt.Print("Digite um numero: ")
	fmt.Scan(&numero3)
	a := numero1*2 + numero2*3 + numero3*5

	fmt.Print("MÃ©dia ponderada:", a/10)
}
